#include <iostream>
#include <cstring>
using namespace std;

int removeConsecutiveDuplicates(string a)
{
    a=a+" ";
    // Write your code here
    for(int i=0;i<a.length()-1;i++)
        {
        if(a[i]!=a[i+1])
            cout<<a[i];
      }
    return 0;
}

int main()
{
    string str;
    cin >> str;
   removeConsecutiveDuplicates(str);
   
    return 0;
}

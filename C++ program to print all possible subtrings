#include <iostream>
#include <cstring>
using namespace std;

void printSubstrings(string input) {
    //Write your code here
    for(int j=0;j<input.length();j++){
      
    for(int i=0;i<input.length();i++)
    {
        
            for(int k=j;k<=i;k++)
            {
                cout<<input[k];
            }cout<<endl;
        }
    
}}

int main() {
    string input;
    getline(cin, input);
    printSubstrings(input);
    return 0;
}
